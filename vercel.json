{
  "version": 2,
  "builds": [
    { "src": "api/*.go", "use": "@vercel/go" }
  ],
  "routes": [
    { "src": "/api/", "dest": "/api/index.go"},
    { "src": "/api/discount", "dest": "/api/customer_discount.go"},
    { "src": "/api/loyaltyCard", "dest": "/api/loyalty_card.go"},
    { "src": "/api/customers", "dest": "/api/all_customers.go"}

  ]
}
